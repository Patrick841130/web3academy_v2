{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://web3academyv2-production.up.railway.app/api/:path*"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}